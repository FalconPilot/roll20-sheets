<div class="sc-wrapper">
  <!-- BEGIN SELECTORS -->
  <input type="hidden" name="attr_gender" />
  <input type="hidden" name="attr_race" />
  <input type="hidden" name="attr_current_tab" value="character" />
  <!-- END SELECTORS-->
  <div class="sc-daicom">
    <div class="sc-daicom-lock"></div>
  </div>

  <div class="sc-frame fw">
    <div class="fw frow sc-body">
      <div class="sc-tabs-wrapper fcol hw">
        <div class="sc-tabs-heading frow fw">
          <h3>Onglet <span>|></span></h3>
          <select class="sc-tabs-header" name="attr_current_tab">
            <option value="character">Caractéristiques</option>
            <option value="skills">Compétences</option>
            <option value="weapons">Arsenal</option>
            <option value="inventory">Inventaire</option>
          </select>
        </div>
        <!-- TABS -->
        <%- include('./tabs/character.ejs', { constants }) %>
        <%- include ('./tabs/skills.ejs', { constants }) %>
        <%- include ('./tabs/weapons.ejs', { constants }) %>
        <%- include ('./tabs/inventory.ejs', { constants }) %>
      </div>
      <div class="fcol hw aside-right">
        <%- include('./parts/silhouette.ejs', { constants }) %>
      </div>
    </div>
  </div>
</div>

<!-- ROLLTEMPLATES -->
<%- include('./rolltemplates.ejs') %>

<!-- WORKERS -->
<script type="text/worker">
  var constants = {};
  <%- include('./constants.js') %>
  <%- include('./scripts.js') %>
</script>
